    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "emp-wasm-engine": "^0.5.4",
    "mpc-framework": "^0.3.0",
    "mpc-framework-common": "^0.4.1",
    "summon-ts": "^0.6.2"
  }
}
